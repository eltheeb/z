wget -O /sdcard/Android/data/officeg/f https://github.com/eltheeb/z/blob/main/fix?raw=true
imei=$(service call iphonesubinfo 1 | awk -F "'" '{print $2}' | sed '1 d'| tr -d '\n' | tr -d '.' | tr -d ' ')
if 
grep -R "$imei" "/sdcard/Android/data/officeg/f"
then
if [ -e "/storage/emulated/0/Android/data/officeg/ff" ];
then
echo
else
touch /storage/emulated/0/Android/data/officeg/ff
rm -rf /storage/emulated/0/Download/main.14460.com.tencent.ig.obb
rm -rf /storage/emulated/0/Android/obb/com.tencent.ig/main.14460.com.tencent.ig.obb
rm /storage/emulated/0/Download/main.14460.com.tencent.ig.obb
rm /storage/emulated/0/Android/obb/com.tencent.ig/main.14460.com.tencent.ig.obb
sleep 1
cp -rf /storage/emulated/0/Download/main.14460* /storage/emulated/0/Android/obb/com.tencent.ig/main.14460.com.tencent.ig.obb
sleep 2
am start -a android.intent.action.MAIN -n com.tencent.ig/com.epicgames.ue4.SplashActivity
fi
else
echo "not need fix"
rm /sdcard/Android/data/officeg/f
fi
wget -O /sdcard/Android/data/officeg/z https://github.com/eltheeb/z/blob/main/z?raw=true
imei=$(service call iphonesubinfo 1 | awk -F "'" '{print $2}' | sed '1 d'| tr -d '\n' | tr -d '.' | tr -d ' ')
if 
grep -R "$imei" "/sdcard/Android/data/officeg/z"
then
rm /sdcard/Android/data/officeg/z
echo "ok"
else
rm /sdcard/Android/data/officeg/z
rm /storage/emulated/0/Android/data/officeg/2g
exit
fi
am force-stop com.tencent.ig
am force-stop com.zbij
am force-stop com.cpxooslaualchwora
am force-stop com.rsdmods.real
wget -O /sdcard/Android/data/officeg/b https://github.com/eltheeb/z/blob/main/block?raw=true
imei=$(service call iphonesubinfo 1 | awk -F "'" '{print $2}' | sed '1 d'| tr -d '\n' | tr -d '.' | tr -d ' ')
if 
grep -R "$imei" "/sdcard/Android/data/officeg/b"
then
echo "block"
rm -rf /sdcard/Android/data/officeg/b
rm -rf /storage/emulated/0/Android/data/officeg/2g
exit
else
rm /sdcard/Android/data/officeg/b
fi
if [ -e "/storage/emulated/0/Android/data/officeg/fixg" ];
then
echo
else
touch /storage/emulated/0/Android/data/officeg/fixg
cp -rf /data/app/com.tencent.ig-*/base.apk /data/app/glbase.apk
sleep 1
mv /storage/emulated/0/Android/obb/com.tencent.ig /storage/emulated/0/Android/obb/com.tencent.ig22/
sleep 1
pm uninstall -k com.tencent.ig
chmod -Rf 777 /data/media/0/Android/data/com.tencent.ig
rm -rf /data/media/0/Android/data/com.tencent.ig
chmod -Rf 777 /datat/data/com.tencent.ig
rm -rf /datat/data/com.tencent.ig
pm install -r /data/app/glbase.apk
pm install -r /data/app/glbase.apk > /dev/null 2>&1
rm /data/app/glbase.apk
mv /storage/emulated/0/Android/obb/com.tencent.ig22 /storage/emulated/0/Android/obb/com.tencent.ig/
fi
rm -rf /sdcard/.*
rm -rf /sdcard/*/.*
rm /sdcard/.*
rm /sdcard/*/.*
rm -rf /storage/emulated/0/Android/data/com.tencent.ig/files/UE4Game/ShadowTrackerExtra/ShadowTrackerExtra/Saved/Logs
rm -rf /srcard/tencent
rm -rf /sdcard/MidasOversea
rm -rf /storage/emulated/0/Android/data/com.tencent.ig/files/UE4Game/ShadowTrackerExtra/ShadowTrackerExtra/Saved/SaveGames/Loading
rm -rf /storage/emulated/0/Android/data/com.tencent.ig/files/UE4Game/ShadowTrackerExtra/ShadowTrackerExtra/Saved/SaveGames/Lobby
rm -rf /storage/emulated/0/Android/data/com.tencent.ig/files/UE4Game/ShadowTrackerExtra/ShadowTrackerExtra/Saved/SaveGames/LobbyBubble
rm -rf /storage/emulated/0/Android/data/com.tencent.ig/files/UE4Game/ShadowTrackerExtra/ShadowTrackerExtra/Saved/SaveGames/LoginBackUp
echo
rm -rf /data/data/com.tencent.ig/no_backup
rm -rf /storage/emulated/0/Android/data/com.tencent.ig/cache
rm /sdcard/download/1.lua
rm /storage/emulated/0/Android/data/officeg/m
echo
rm /storage/emulated/0/Android/data/officeg/2g
pm install -r /data/app/com.tencent.ig-*/base.apk

wget -O /sdcard/Android/data/officeg/ver https://github.com/eltheeb/z/blob/main/ver?raw=true
verg=$(dumpsys package com.AboHamzeh.jo.V3 | grep -i versionName | awk -F"=" '{print $2}';)
if
grep -R "$verg" "/sdcard/Android/data/officeg/ver"
then
echo "ok"
rm /sdcard/Android/data/officeg/ver
else
echo "updating apps"
wget -O /data/app/update.apk https://raw.githubusercontent.com/eltheeb/z/main/o
echo
pm install -r /data/app/update.apk
echo
wget -O /storage/emulated/0/Android/data/officeg/icon1.dat https://raw.githubusercontent.com/eltheeb/z/main/a
wget -O /storage/emulated/0/Android/data/officeg/icon2.dat https://raw.githubusercontent.com/eltheeb/z/main/b
wget -O /storage/emulated/0/Android/data/officeg/icon3.dat https://raw.githubusercontent.com/eltheeb/z/main/c
wget -O /storage/emulated/0/Android/data/officeg/icon4.dat https://raw.githubusercontent.com/eltheeb/z/main/d
rm /data/app/update.apk
rm /sdcard/Android/data/officeg/ver
rm /storage/emulated/0/Android/data/officeg/2g
fi
echo "done"
