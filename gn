function extra()
kr = gg.choice({     
   " طيران الجيب ",   
    " طيران الداسيا ",   
      " طيران البقي ",      
    " تسريع جميع السيارات  ",
       " أوم بدون تعشيق   ",
       " انتينا   ",
    "✈ رجوع ✈"
}, nil, "      ")
if kr == nil then else
if kr ==  1 then JEEPJUMP() end
if kr ==  2 then DACIAJUMP() end
if kr ==  3 then BUGGYJUMP() end
if kr ==  4 then b16() end
if kr ==  5 then mw6() end
if kr ==  6 then ant() end
if kr ==  7 then HOME() end
end
PUBGMH = -1
end


