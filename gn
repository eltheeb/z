function HOME()
  SAFEON = gg.multiChoice({
    "⟦☠︎︎️⟧ أزالة العشب لوبي ",
    "⟦☠︎︎️⟧ ايم بوت  ",
    "⟦☠︎︎️⟧ ثبات 50  ",
    "⟦☠︎︎️⟧ ثبات 100  ",
    "⟦☠︎︎️⟧ هيدشوت+ماجيك ",
    "⟦☠︎︎️⟧ منضور أيباد ",
    "⟦☠︎︎️⟧ أزالة الضباب ",
    "⟦☠︎︎️⟧ القفز التلقائي ",
    "⟦☠︎︎️⟧ الخيارات الأضافيه",
    "❌خروج❌"
 }, nil, "⚜Abo Hamzeh VIP⚜")
  if SAFEON == nil then
  else
    if SAFEON[1] == true then
      lng()
    end
   if SAFEON[2] == true then
     aim()
    end
    if SAFEON[3] == true then
      lnr50()
    end
    if SAFEON[4] == true then
      lngl()
    end
    if SAFEON[5] == true then
      HSHOT100()
    end
        if SAFEON[6] == true then
      lipv()
    end
    if SAFEON[7] == true then
      lpov()
    end
    if SAFEON[8] == true then
      aj()
    end
    if SAFEON[9] == true then
      extra()
    end
        if SAFEON[10] == true then
      EXIT()
    end
  end
  PUBGMH = -1
end


